[user]
    email = rhysormond@gmail.com
    name = rhys
[core]
    editor = vim
    excludesfile = ~/.gitignore_global
    filemode = false
[push]
    default = simple
[pager]
    branch = false
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
[color]
    ui = true
[alias]
    graph = log --all --decorate --oneline --graph
